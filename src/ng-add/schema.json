{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "ngAdd",
  "title": "My Full Schematics Schema",
  "type": "object",
  "properties": {
    "srcDir": {
      "type": "string",
      "default": "./testSrc/"
    },
    "appDir": {
      "type": "string",
      "default": "./testSrc/app/"
    },
    "clientId": {
      "type": "string",
      "x-prompt": "What is your clientId?"
    },
    "tenantId": {
      "type": "string",
      "x-prompt": "What is your tenantId?"
    },
    "projectName": {
      "type": "string",
      "default": "myApp",
      "x-prompt": "What is your project name?"
    },
    "description": {
      "type": "string",
      "default": "Test and create stuff",
      "x-prompt": "What is your project description?"
    },
    "url": {
      "type": "string",
      "default": "test.servier.com",
      "x-prompt": "What is your project website?"
    }
  },
  "required": ["clientId", "tenantId", "projectName", "description", "url"]
}
